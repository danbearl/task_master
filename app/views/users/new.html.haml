%h1 Create new account

=form_for user do |f|
  .field
    = f.label :email
    %br
    = f.text_field :email
  .field
    = f.label :password
    %br
    = f.text_area :password
  .field
    = f.label :password_confirmation
    %br
    = f.text_area :password_confirmation

  = submit_tag "Sign Up"
